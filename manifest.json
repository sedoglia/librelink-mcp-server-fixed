{
  "manifest_version": "0.2",
  "name": "librelink-mcp-server-fixed",
  "version": "1.3.0",
  "description": "MCP server for FreeStyle LibreLink CGM data - Fixed for API v4.16.0 with secure credential storage",
  "author": {
    "name": "sedoglia",
    "email": "sedoglia@gmail.com"
  },
  "homepage": "https://github.com/sedoglia/librelink-mcp-server-fixed",
