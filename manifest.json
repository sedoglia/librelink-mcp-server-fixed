{
  "manifest_version": "0.2",
  "name": "librelink-mcp-server",
  "version": "1.3.0",
  "description": "MCP server for FreeStyle LibreLink CGM data - Fixed for API v4.16.0 with secure credential storage",
  "author": {
    "name": "sedoglia",
    "email": "sedoglia@gmail.com"
  },
  "homepage": "https://github.com/sedoglia/librelink-mcp-server",
  "documentation": "https://github.com/sedoglia/librelink-mcp-server/blob/main/README.en.md",
  "support": "https://github.com/sedoglia/librelink-mcp-server/issues",
  "icon": ".\\LibreLink.png",
  "server": {
    "type": "node",
    "entry_point": "dist/index.js",
    "mcp_config": {
      "command": "node",
      "args": [
        "${__dirname}/dist/index.js"
      ],
      "env": {}
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sedoglia/librelink-mcp-server"
  }
}
